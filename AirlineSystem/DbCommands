create table Users(user_id varchar(20),email varchar(30),password varchar(20), phone_number varchar(10),gender varchar(10));

create table Airlines(flight_number int, airlines_id varchar(20), source varchar(20), destination varchar(20), date date, departure time, arrival time, base_price float);

create table Bookings(email varchar(30), flight_num int);

